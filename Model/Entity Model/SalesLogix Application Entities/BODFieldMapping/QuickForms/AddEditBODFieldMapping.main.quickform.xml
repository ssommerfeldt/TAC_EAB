      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>var fieldMapping = BindingSource.Current as IBODFieldMapping;
bool isClone = false;
bool isReadOnly=false;
//when cloning a Bod Mapping don't allow for system properties to be modifiable, when copying a Bod Mapping system properties are modifiable
if (DialogService.DialogParameters.ContainsKey("CloneFieldMapping"))
{
    isClone = true;
    var cloneMap = Sage.Platform.EntityFactory.GetById&lt;IBODFieldMapping&gt;(DialogService.DialogParameters["CloneFieldMapping"].ToString());
    if (cloneMap != null)
    {
        fieldMapping.CloneBodFieldMapping(cloneMap, false);
    }
    DialogService.DialogParameters.Remove("CloneFieldMapping");
}
else if (DialogService.DialogParameters.ContainsKey("CopyFieldMapping"))
{
    var copyMap = Sage.Platform.EntityFactory.GetById&lt;IBODFieldMapping&gt;(DialogService.DialogParameters["CopyFieldMapping"].ToString());
    if (copyMap != null)
    {
        fieldMapping.CopyBodFieldMapping(copyMap, false);
    }
    DialogService.DialogParameters.Remove("CopyFieldMapping");
}
else if (DialogService.DialogParameters.ContainsKey("IsExtension"))
{
    fieldMapping.IsExtension = true;
    DialogService.DialogParameters.Remove("IsExtension");
}
if(string.IsNullOrWhiteSpace(txtFieldMapping.Text))
{
fieldMapping.IsExtension = true;
}
if((fieldMapping.IsExtension==true) &amp;&amp; (fieldMapping.IsCustom==true))
{
	isReadOnly = false;	 
}
else if((fieldMapping.IsExtension==false) &amp;&amp; (fieldMapping.IsCustom==true))
{
isReadOnly=true;
}
txtEntity.ReadOnly = isReadOnly;
txtParentEntity.ReadOnly = isReadOnly;
txtProperty.ReadOnly = isReadOnly;</CSharpCodeSnippet>
