txtDefaultValue.ReadOnly = !isCustom;
chkIncludeInSearch.Enabled = isCustom;
pklSearchValue.ReadOnly = !isCustom;
chkActive.Enabled = isCustom;
btnSave.Visible = isCustom;
btnDelete.Visible = isCustom;

var histoyEntity = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.Application.IEntityHistoryService&gt;();
if (histoyEntity != null)
{
   foreach (var type in histoyEntity)
   {
      if (!string.IsNullOrEmpty(type.EntityId.ToString()) &amp;&amp; type.EntityType == typeof(IBackOffice))
      {
        fieldMapping.BackOffice = Sage.Platform.EntityFactory.GetById&lt;IBackOffice&gt;(type.EntityId);
        break;
       }
   }
