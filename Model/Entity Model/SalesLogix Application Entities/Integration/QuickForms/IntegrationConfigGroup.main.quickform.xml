            <MergeControlId />
            <OnClick>function () {
      var selectedItems = this.grid.getSelectedRowData();
    if (selectedItems.length === 0) {
        Sage.UI.Dialogs.showInfo(IntegrationConfigGroupResources['selectConfigGroup']);
    } else {
        var item = selectedItems[0];        
        var service = sDataServiceRegistry.getSDataService('dynamic');
        var request = new Sage.SData.Client.SDataServiceOperationRequest(service)
                      .setResourceKind("integrationConfigGroups")
                      .setOperationName("CopyConfigGroup");
        var entry = {
            "$name": "CopyConfigGroup",
            "request": {
                "IntegrationConfigGroupId": item.$key
            }
        };
        request.execute(entry, {
            async: false,
            success: function (result) {
                grid.refresh();
            },
            failure: function (result) {
                Sage.UI.Dialogs.showError(result);
            }
        });
    }
}</OnClick>
