			                 var appId = item.AppIdMapping.$key;			                 
			                 request.setResourceSelector(dojo.string.substitute("'${0}'", [appId]));
			                 var returnValue = null;			                
			                 request.read({
			                     async: false,
			                     success: function(entry) {			                        
			                         var createDate = null;
			                         if(Sage.Utility.Convert.isDateString(entry.CreateDate)) {
			                             createDate = Sage.Utility.Convert.toDateFromString(entry.CreateDate);
			                             createDate = dojo.date.locale.format(createDate, { locale: Sys.CultureInfo.CurrentCulture.name });
			                         }			                         			                         
			                         returnValue = createDate;
			                     },
			                     failure: function() {
			                         console.log("SDataSingleResourceRequest failed on syncLastDateView");
			                     },
			                 });
			                 return returnValue;
			             } </CustomFormatter>
