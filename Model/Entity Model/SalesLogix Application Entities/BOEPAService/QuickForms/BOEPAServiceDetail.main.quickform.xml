      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>Sage.Platform.WebPortal.EntityPage page = Page as Sage.Platform.WebPortal.EntityPage;
IBOEPAService boePAService = BindingSource.Current as IBOEPAService;
if(!string.IsNullOrWhiteSpace(boePAService.Password))
{
txtPassword.Attributes.Add("value", boePAService.Password);
}
var isCustom = (bool)boePAService.IsCustom;
btnDelete.Visible = isCustom;
bool copiedService = isCustom &amp;&amp; boePAService.Id.ToString() == boePAService.STDBOEPAServiceID;
if (isCustom || copiedService)
{
    cboAuthType.Attributes.Remove("disabled");
    cboMethod.Attributes.Remove("disabled");
}
txtServiceName.ReadOnly = !copiedService;
txtEndPoint.ReadOnly = !isCustom;
pklServiceType.ReadOnly = !isCustom;
txtAction.ReadOnly = !copiedService;
txtDescription.ReadOnly = !isCustom;
txtUserName.ReadOnly = !isCustom;
txtPassword.ReadOnly = !isCustom;
chkActive.Enabled = isCustom;
var histoyEntity = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.Application.IEntityHistoryService&gt;();
if (histoyEntity != null)
{
   foreach (var type in histoyEntity)
   {
      if (!string.IsNullOrEmpty(type.EntityId.ToString()) &amp;&amp; type.EntityType == typeof(IBackOffice))
      {
		lueBackOffice.LookupResultValue=Sage.Platform.EntityFactory.GetById&lt;IBackOffice&gt;(type.EntityId);
        //mapping.BackOffice = Sage.Platform.EntityFactory.GetById&lt;IBackOffice&gt;(type.EntityId);
        break;
       }
   }
}
if(lueBackOffice.LookupResultValue==null)
{
lueBackOffice.LookupResultValue=boePAService.BackOffice;
}
var selectedString = string.Empty;
var duplicateItems = new List&lt;ListItem&gt;();
foreach (ListItem item in cboAuthType.Items)
{
    if (item.Selected)
    {
        selectedString = item.Text;
    }
    if (item.Value == item.Text)
    {
        duplicateItems.Add(item);
    }
}
foreach (ListItem item in duplicateItems)
{
    cboAuthType.Items.Remove(item);
}
if (!string.IsNullOrEmpty(selectedString))
{
    cboAuthType.Items.FindByText(selectedString).Selected = true;
}</CSharpCodeSnippet>
