	var salesOrderItem = Sage.Platform.EntityFactory.GetById&lt;ISalesOrderItem&gt;(detailId);
	salesOrderItem.ConfigProductItem(headId, detailId, string.Empty, false, salesOrderItem.Status,string.Empty);
	var url = Request.Url.AbsoluteUri;
	var index = url.IndexOf('&amp;');
	if(index &gt; 0)
	{
		url = url.Substring(0,index);
		Response.Redirect(url);
	}	
}
