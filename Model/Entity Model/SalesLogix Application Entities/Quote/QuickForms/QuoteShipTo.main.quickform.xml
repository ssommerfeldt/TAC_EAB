          <CSharpCodeSnippet>var quote = BindingSource.Current as IQuote;
if (quote == null) return;
var mappingService = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.SData.IAppIdMappingService&gt;(true);
bool isIntegrationEnabled = mappingService.IsIntegrationEnabled();
ctrlstErpShipTo.Visible = isIntegrationEnabled;
lueShipToContact.Visible = !isIntegrationEnabled;
lueShipToContact_lbl.Visible = !isIntegrationEnabled;
bool selected = false;
lstbxShippingAddress.Items.Clear();
var closed = quote.IsClosed();
lueCarrier.Visible = isIntegrationEnabled;
lueCarrier_lbl.Visible = isIntegrationEnabled;
pklShipVia.Visible = !isIntegrationEnabled;
pklShipVia_lbl.Visible = !isIntegrationEnabled;
if (quote.ShippingContact != null || quote.ShipTo != null)
{
    string contactId = string.Empty;
    if (isIntegrationEnabled)
    {
        if (quote.ShipTo != null) contactId = quote.ShipTo.Id.ToString();
    }
    else
    {
        if (quote.ShippingContact != null) contactId = quote.ShippingContact.Id.ToString();
    }
    if (!string.IsNullOrEmpty(contactId))
    {
        var shippingAddresses =
            Sage.Platform.EntityFactory.GetRepository&lt;IAddress&gt;().FindByProperty(
                "EntityId", contactId);
        if (shippingAddresses != null)
        {
            foreach (IAddress address in shippingAddresses)
            {
                var shippingItem = new ListItem {Text = address.Description, Value = address.Id.ToString()};
                if (!selected)
                {
                    shippingItem.Selected = Equals(address.Description, quote.ShippingAddress.Description);
                    selected = shippingItem.Selected;
                }
                lstbxShippingAddress.Items.Add(shippingItem);
            }
        }
    }
}
if (isIntegrationEnabled &amp;&amp; !closed)
{
    var fls = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.Security.IFieldLevelSecurityService&gt;(true);
    if (fls != null)
    {
        var access = fls.GetAccessForProperty(quote, "Address1");
        addShipTo.ReadOnly = (access == Sage.Platform.Security.FieldAccess.ReadOnly);
    }
}
if (closed)
{
	addShipTo.ReadOnly = true;
	btnAddShipTo.Visible = false;
	 lueShipTo.ReadOnly = true;
	lueShipToContact.ReadOnly = true;
	lstbxShippingAddress.Enabled = false;
	chkEarlyShip.Enabled = false;
	chkPartialShip.Enabled = false;
	chkDropShip.Enabled = false;
	pklShipVia.Enabled = false;
	lueCarrier.ReadOnly = true;
	tbrShiptoSave.Visible = false;
}
</CSharpCodeSnippet>
