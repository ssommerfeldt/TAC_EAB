          <CSharpCodeSnippet>var mappingService = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.SData.IAppIdMappingService&gt;(true);
bool isIntegrationEnabled = mappingService.IsIntegrationEnabled();
ctrlstErpBillTo.Visible = isIntegrationEnabled;
lueBillToContact.Visible = !isIntegrationEnabled;
lueBillToContact_lbl.Visible = !isIntegrationEnabled;
pklPaymentMethod.Visible = isIntegrationEnabled;
pklPaymentMethod_lbl.Visible = isIntegrationEnabled;
var quote = BindingSource.Current as IQuote;
if (quote == null) return;
bool selected = false;
lstbxBillingAddress.Items.Clear();
var closed = quote.IsClosed();
if (quote.BillingContact != null || quote.BillTo != null)
{
    string contactId = string.Empty;
    if (isIntegrationEnabled)
    {
        if (quote.BillTo != null) contactId = quote.BillTo.Id.ToString();
    }
    else
    {
        if (quote.BillingContact != null) contactId = quote.BillingContact.Id.ToString();
    }
    if (!string.IsNullOrEmpty(contactId))
    {
        var billingAddresses =
            Sage.Platform.EntityFactory.GetRepository&lt;IAddress&gt;().FindByProperty(
                "EntityId", contactId);
        if (billingAddresses != null)
        {
            foreach (IAddress address in billingAddresses)
            {
                var billingItem = new ListItem {Text = address.Description, Value = address.Id.ToString()};
                if (!selected &amp;&amp; quote.BillingAddress != null)
                {
                    billingItem.Selected = Equals(address.Description, quote.BillingAddress.Description);
                    selected = billingItem.Selected;
                }
                lstbxBillingAddress.Items.Add(billingItem);
            }
        }
    }
}
if (isIntegrationEnabled)
{
    var fls = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.Security.IFieldLevelSecurityService&gt;(true);
    if (fls != null)
    {
        var access = fls.GetAccessForProperty(quote, "Address1");
        addBilling.ReadOnly = (access == Sage.Platform.Security.FieldAccess.ReadOnly);
    }
}

if (closed)
{
    addBilling.ReadOnly = true;
    btnAddErpBillTo.Visible = false;
    lueErpBillTo.ReadOnly = true;
	txtAttention.ReadOnly = true;
	lueBillToContact.ReadOnly = true;
	lstbxBillingAddress.Enabled = false;
	addBilling.ReadOnly = true;
	tbrBilltoSave.Visible = false;
}</CSharpCodeSnippet>
