        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>IBOEPAMapping serviceMap = BindingSource.Current as IBOEPAMapping;
var isCustom = serviceMap.IsCustom==true?true:false;
var isCopied = serviceMap.IsExtension==true?true:false;
tbrDelete.Visible = isCustom;

cboEntity.Items.Clear();
var ts = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;System.ComponentModel.Design.ITypeDiscoveryService&gt;();
var types = ts.GetTypes(typeof(Sage.Platform.Orm.Interfaces.IPersistentEntity), true);
foreach (Type entity in types)
{
    string name = entity.Name;
    if (name.StartsWith("I"))
    {
        name = name.Remove(0, 1);
    }
    cboEntity.Items.Add(new ListItem { Text = name, Value = name, Selected = serviceMap.Entity == name });
}

if(isCopied)
{
cboEntity.Attributes.Remove("disabled");
txtField.ReadOnly=false;
}
txtName.ReadOnly=!isCustom;
txtDescription.ReadOnly=!isCustom;
txtEntityXPath.ReadOnly=!isCustom;
txtPropertyXPath.ReadOnly=!isCustom;
chkActive.Enabled = isCustom;
chkRequired.Enabled = isCustom;</CSharpCodeSnippet>
        </CSharpSnippetActionItem>
      </Action>
