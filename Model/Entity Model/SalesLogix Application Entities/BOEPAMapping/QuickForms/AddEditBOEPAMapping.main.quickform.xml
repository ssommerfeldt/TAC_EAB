          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>IBOEPAMapping boePAMapping = BindingSource.Current as IBOEPAMapping; 

bool copyMapping = false;
bool isReadOnly = false;
bool isClone = false;

var isCustom = boePAMapping.IsCustom==true?true:false;

//when cloning a Bod Mapping don't allow for system properties to be modifiable, when copying a Bod Mapping system properties are modifiable
if (DialogService.DialogParameters.ContainsKey("CloneServiceMapping"))
{
	isCustom=true;
    var cloneMap = Sage.Platform.EntityFactory.GetById&lt;IBOEPAMapping&gt;(DialogService.DialogParameters["CloneServiceMapping"].ToString());
    if (cloneMap != null)
    {
        boePAMapping.CloneBOEPAMapping(cloneMap, false);
    }
    DialogService.DialogParameters.Remove("CloneServiceMapping");
}
else if (DialogService.DialogParameters.ContainsKey("CopyServiceMapping"))
{
    var copyMap = Sage.Platform.EntityFactory.GetById&lt;IBOEPAMapping&gt;(DialogService.DialogParameters["CopyServiceMapping"].ToString());
    if (copyMap != null)
    {
        boePAMapping.CopyServiceMapping(copyMap, false);
    }
    copyMapping = true;
    DialogService.DialogParameters.Remove("CopyServiceMapping");
}
if(string.IsNullOrWhiteSpace(txtName.Text))
{
boePAMapping.IsExtension = true;
copyMapping = true;
}
if(boePAMapping.IsExtension==true)
{
	copyMapping = true;
}
if (copyMapping)
{
    cboEntity.Attributes.Remove("disabled");
	
}


txtName.ReadOnly = !isCustom;
txtEntityXPath.ReadOnly = !isCustom;
txtPropertyXpath.ReadOnly = !isCustom;
txtDescription.ReadOnly=!isCustom;
txtEntityXPath.ReadOnly=!isCustom;
txtProperty.ReadOnly=!copyMapping;

cboEntity.Items.Clear();
var ts = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;System.ComponentModel.Design.ITypeDiscoveryService&gt;();
var types = ts.GetTypes(typeof(Sage.Platform.Orm.Interfaces.IPersistentEntity), true);
foreach (Type entity in types)
{
    string name = entity.Name;
    if (name.StartsWith("I"))
    {
        name = name.Remove(0, 1);
    }
    cboEntity.Items.Add(new ListItem { Text = name, Value = name, Selected = boePAMapping.Entity == name });
}</CSharpCodeSnippet>
