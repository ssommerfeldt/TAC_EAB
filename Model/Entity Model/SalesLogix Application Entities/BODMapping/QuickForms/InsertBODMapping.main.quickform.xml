      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>var mapping = BindingSource.Current as IBODMapping;
bool copyMapping = false;
//mapping.IsExtension = true;
//when cloning a Bod Mapping don't allow for system properties to be modifiable, when copying a Bod Mapping system properties are modifiable
if (DialogService.DialogParameters.ContainsKey("CloneMapping"))
{
    var cloneMap = Sage.Platform.EntityFactory.GetById&lt;IBODMapping&gt;(DialogService.DialogParameters["CloneMapping"].ToString());
    if (cloneMap != null)
    {
        mapping.CloneBodMapping(cloneMap, false);
    }
    DialogService.DialogParameters.Remove("CloneMapping");
}
else if (DialogService.DialogParameters.ContainsKey("CopyMapping"))
{
    var copyMap = Sage.Platform.EntityFactory.GetById&lt;IBODMapping&gt;(DialogService.DialogParameters["CopyMapping"].ToString());
    if (copyMap != null)
    {
        mapping.CopyBodMapping(copyMap, false);
    }
    copyMapping = true;
    DialogService.DialogParameters.Remove("CopyMapping");
}
else if (DialogService.DialogParameters.ContainsKey("IsExtension"))
{
	mapping.IsExtension = true;
	DialogService.DialogParameters.Remove("IsExtension");
}
if (!copyMapping &amp;&amp; !string.IsNullOrWhiteSpace(txtNoun.Text))
{
    txtNoun.ReadOnly = true;
    cboEntity.Attributes.Add("disabled", string.Empty);
    //cboParentEntity.Attributes.Add("disabled", string.Empty);
}
if(string.IsNullOrWhiteSpace(txtNoun.Text))
{
mapping.IsExtension = true;
}
if((mapping.IsExtension==true) &amp;&amp; (mapping.IsCustom==true))
{
	txtNoun.ReadOnly = false;
    cboEntity.Attributes.Remove("disabled");
}
cboEntity.Items.Clear();
cboEntity.Items.Add(new ListItem { Text = "", Value = "" });
var ts = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;System.ComponentModel.Design.ITypeDiscoveryService&gt;();
var types = ts.GetTypes(typeof(Sage.Platform.Orm.Interfaces.IPersistentEntity), true);
foreach (Type entity in types)
{
    string name = entity.Name;
    if (name.StartsWith("I"))
    {
        name = name.Remove(0, 1);
    }
    cboEntity.Items.Add(new ListItem { Text = name, Value = name, Selected = mapping.EntityName == name });
    if (mapping.EntityName == name)
    {
        cboParentEntity.Items.Clear();
        cboParentEntity.Items.Add(new ListItem { Text = "&lt;None&gt;", Value = "" });
        var properties = entity.GetProperties();
        foreach (var property in properties)
        {
            if (property.PropertyType.IsInterface)
            {
                cboParentEntity.Items.Add(new ListItem { Text = property.Name, Value = property.Name, Selected = mapping.RecordOwner == property.Name });
            }
        }
        if (mapping.EntityName == "Account")
            cboParentEntity.Items.Add(new ListItem { Text = "Account", Value = "ParentId#Account", Selected = mapping.RecordOwner == "ParentId#Account" });
    }
}
cboEntity.Items.Add(new ListItem { Text = "PickList", Value = "PickList", Selected = mapping.EntityName == "PickList" });
if (mapping.EntityName == "PickList")
{
    cboParentEntity.Items.Clear();
    cboParentEntity.Items.Add(new ListItem { Text = "&lt;None&gt;", Value = "", Selected = true });
}</CSharpCodeSnippet>
