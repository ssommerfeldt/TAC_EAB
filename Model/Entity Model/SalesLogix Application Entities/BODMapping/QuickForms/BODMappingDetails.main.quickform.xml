      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload1</MethodName>
          <CSharpCodeSnippet>var mapping = BindingSource.Current as IBODMapping;
var isCustom = (bool)mapping.IsCustom;
var isSOR = mapping.SystemOfRecord == Saleslogix.Integration.BOE.Common.Constants.SystemOfRecord.CRM;
var isExtension = mapping.IsExtension ?? false;
btnDelete.Visible = isCustom;
lueBackOffice.ReadOnly = !isExtension;
lueBackOffice.EnableLookup = isExtension;
txtNoun.ReadOnly = !isExtension;
txtBODMappingName.ReadOnly = !isCustom &amp;&amp; !isExtension;
txtHandlerClass.ReadOnly = (!isCustom &amp;&amp; !isExtension) || isSOR;
txtUniqueIDNode.ReadOnly = (!isCustom &amp;&amp; !isExtension) || isSOR;
txtUniqueIDXPath.ReadOnly = (!isCustom &amp;&amp; !isExtension) || isSOR;
pklSystemRecord.ReadOnly = !isCustom &amp;&amp; !isExtension;
pklDeleteBehavior.ReadOnly = !isCustom &amp;&amp; !isExtension;
chkGenOutTemplate.Enabled = isCustom &amp;&amp; !isExtension;
txtPriority.ReadOnly = !isCustom &amp;&amp; !isExtension;
chkActive.Enabled = isCustom;
btnSave.Visible = isCustom || isExtension;
if (!isCustom)
{
    cboOutboundCreate.Attributes.Add("disabled", string.Empty);
    cboOutboundDelete.Attributes.Add("disabled", string.Empty);
    cboOutboundUpdate.Attributes.Add("disabled", string.Empty);
}
if (isExtension)
{
    cboEntityName.Attributes.Remove("disabled");
    cboRecordOwner.Attributes.Remove("disabled");
    cboOutboundCreate.Attributes.Remove("disabled");
    cboOutboundDelete.Attributes.Remove("disabled");
    cboOutboundUpdate.Attributes.Remove("disabled");
}

if((mapping.IsExtension==false) &amp;&amp; (mapping.IsCustom==true))
{
    txtNoun.ReadOnly = true;
    cboEntityName.Attributes.Add("disabled", string.Empty);
    cboRecordOwner.Attributes.Remove("disabled");
}</CSharpCodeSnippet>
