      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>Sage.Entity.Interfaces.IBackOffice backoffice = ((Sage.SalesLogix.Entities.BODMapping)this.BindingSource.Current).BackOffice;
Sage.Platform.WebPortal.Workspaces.Tab.TabWorkspace tabs = this.ParentWorkItem.Workspaces["TabControl"] as Sage.Platform.WebPortal.Workspaces.Tab.TabWorkspace;
//tabs.Hide("CustomFieldMappings", (string.Compare(backoffice.STDBackOfficeID.ToString(),backoffice.Id.ToString())==0));
var mapping = BindingSource.Current as IBODMapping;
var histoyEntity = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.Application.IEntityHistoryService&gt;();
if (histoyEntity != null)
{
   foreach (var type in histoyEntity)
   {
      if (!string.IsNullOrEmpty(type.EntityId.ToString()) &amp;&amp; type.EntityType == typeof(IBackOffice))
      {
		lueBackOffice.LookupResultValue=Sage.Platform.EntityFactory.GetById&lt;IBackOffice&gt;(type.EntityId);
        //mapping.BackOffice = Sage.Platform.EntityFactory.GetById&lt;IBackOffice&gt;(type.EntityId);
        break;
       }
   }
}
if(lueBackOffice.LookupResultValue==null)
{
lueBackOffice.LookupResultValue=mapping.BackOffice;
}

if (mapping != null)
{
    cboEntityName.Items.Clear();
    var ts = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;System.ComponentModel.Design.ITypeDiscoveryService&gt;();
    var types = ts.GetTypes(typeof(Sage.Platform.Orm.Interfaces.IPersistentEntity), true);
    foreach (Type entity in types)
    {
        string name = entity.Name;
        if (name.StartsWith("I"))
        {
            name = name.Remove(0, 1);
        }
        cboEntityName.Items.Add(new ListItem { Text = name, Value = name, Selected = mapping.EntityName == name });
        if (mapping.EntityName == name)
        {
            //load and set the Inherited Owner From
            cboRecordOwner.Items.Clear();
			cboRecordOwner.Items.Add(new ListItem { Text = "&lt;None&gt;", Value = "" });
            var properties = entity.GetProperties();
            foreach (var property in properties)
            {
                if (property.PropertyType.IsInterface)
                {
                    cboRecordOwner.Items.Add(new ListItem { Text = property.Name, Value = property.Name, Selected = mapping.RecordOwner == property.Name });
                }
            }
			if(mapping.EntityName == "Account")
				cboRecordOwner.Items.Add(new ListItem { Text = "Account", Value = "ParentId#Account" ,Selected = mapping.RecordOwner == "ParentId#Account"  });
        }
    }
	cboEntityName.Items.Add(new ListItem { Text = "PickList", Value = "PickList", Selected = mapping.EntityName == "PickList" });
	if (mapping.EntityName == "PickList")
	{
		cboRecordOwner.Items.Clear();
		cboRecordOwner.Items.Add(new ListItem { Text = "&lt;None&gt;", Value = "" ,Selected = true });
	}
}</CSharpCodeSnippet>
