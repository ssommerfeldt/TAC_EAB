            <CSharpSnippetActionItem>
              <MethodName>btnAdd_ClickAction</MethodName>
              <CSharpCodeSnippet>Sage.Entity.Interfaces.IERPBillTo billTo = this.BindingSource.Current as Sage.Entity.Interfaces.IERPBillTo;

bool doAssRoleUnsynced = false;
using (var session = new Sage.Platform.Orm.SessionScopeWrapper())
{
    var dataValue = session.QueryOver&lt;Sage.Entity.Interfaces.ICustomSetting&gt;()
        .Where(i =&gt; i.Category == "Back Office Extension" &amp;&amp; i.Description == "Associate Roles to Unsynced Account")
        .Select(i =&gt; i.DataValue)
        .SingleOrDefault&lt;string&gt;();
    if (!String.IsNullOrEmpty(dataValue))
    {
        bool value;
        bool.TryParse(dataValue, out value);
        doAssRoleUnsynced = value;
    }
}
if(billTo.ErpExtId == null &amp;&amp; !doAssRoleUnsynced)
{
  	DialogService.ShowMessage(GetLocalResourceObject("error_PayfromCanNotAdd").ToString());
}
else
{
	DialogService.SetSpecs(200, 200, 550, 800, "AddEditERPPayFrom");
	DialogService.EntityType = typeof(Sage.Entity.Interfaces.IERPPayFrom);
	DialogService.ShowDialog();
}
</CSharpCodeSnippet>
            </CSharpSnippetActionItem>
