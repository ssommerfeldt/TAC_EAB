                      <MethodName>btnOK_ClickAction</MethodName>
                      <CSharpCodeSnippet>var billTo = BindingSource.Current as Sage.Entity.Interfaces.IERPBillTo;
object parent = GetParentEntity();

billTo.Save();

if (parent is ISalesOrder)
{
    var salesOrder = ((Sage.SalesLogix.Entities.SalesOrder) (parent));
    salesOrder.ErpBillTo = billTo;
    salesOrder.SetSalesOrderBillingAddress(billTo.Address);
	salesOrder.Save();
}
else if (parent is IQuote)
{
    var quote = ((Sage.SalesLogix.Entities.Quote) (parent));
    quote.BillTo = billTo;
    quote.SetQuoteBillingAddress(billTo.Address);
	quote.Save();
}</CSharpCodeSnippet>
