            <CSharpSnippetActionItem>
              <MethodName>pklDataType_ChangeAction</MethodName>
              <CSharpCodeSnippet>	Sage.Entity.Interfaces.ICustomSetting cs = this.BindingSource.Current as Sage.Entity.Interfaces.ICustomSetting;
	
	switch (cs.DataType)
	{
		case "Other":
			cs.DataValidation = "Any Characters";
			break;
		case "Number":
			cs.DataValidation = "Numbers Only";
			break;
		case "True / False":
			cs.DataValidation = "True or False";
			break;
		case "Data":
		case "Picklist":
		default:
			cs.DataValidation = null;
			break;			
	}</CSharpCodeSnippet>
