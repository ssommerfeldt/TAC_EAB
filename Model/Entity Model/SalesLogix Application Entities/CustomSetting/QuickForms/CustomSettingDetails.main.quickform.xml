          <CSharpCodeSnippet>var mappingService = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.Platform.SData.IAppIdMappingService&gt;(true);
var mapping = BindingSource.Current as Sage.Entity.Interfaces.ICustomSetting;
var description = new string[] { "Local CRM Pricing Quote", "Local CRM Pricing SalesOrder", "Local CRM Pricing Opportunity" };
if (mapping != null &amp;&amp; description.Contains(mapping.Description))
{
		if (!mappingService.IsBackOfficeIntegrationEnabled())
		{
			txtCategory.ReadOnly = true;
			txtDescription.ReadOnly = true;
			txtDataValue.ReadOnly = true;
			txtValidation.ReadOnly = true;
			pklDataType.ReadOnly = true;			
			txtVersionNumber.ReadOnly = true;			
		}		
}
if (mappingService.IsBackOfficeIntegrationEnabled())
{
  var enableExchRate = "Enable Dated Exchange Rates";
  if(mapping != null &amp;&amp; enableExchRate.Contains(mapping.Description))
    {
  	  txtDataValue.ReadOnly = true;
	}
	else
	{
	  txtDataValue.ReadOnly = false;
	}
}</CSharpCodeSnippet>
