          <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
            <CSharpSnippetActionItem>
              <MethodName>btnAddBillTo_ClickAction</MethodName>
              <CSharpCodeSnippet>Sage.Entity.Interfaces.IAccount account = this.BindingSource.Current as Sage.Entity.Interfaces.IAccount;

bool doAssRoleUnsynced = false;
using (var session = new Sage.Platform.Orm.SessionScopeWrapper())
{
    var dataValue = session.QueryOver&lt;Sage.Entity.Interfaces.ICustomSetting&gt;()
        .Where(i =&gt; i.Category == "Back Office Extension" &amp;&amp; i.Description == "Associate Roles to Unsynced Account")
        .Select(i =&gt; i.DataValue)
        .SingleOrDefault&lt;string&gt;();
    if (!String.IsNullOrEmpty(dataValue))
    {
        bool value;
        bool.TryParse(dataValue, out value);
        doAssRoleUnsynced = value;
    }
}
if(account.ErpExtId == null &amp;&amp; !doAssRoleUnsynced)
{
  	DialogService.ShowMessage(GetLocalResourceObject("error_BilltoCanNotAdd").ToString());
}
else
{
	DialogService.SetSpecs(200,200,550, 800, "AddEditErpBillTo" );
	DialogService.EntityType = typeof(Sage.Entity.Interfaces.IERPBillTo); 
	DialogService.ShowDialog();
}</CSharpCodeSnippet>
            </CSharpSnippetActionItem>
          </Action>
          <IsDialogCloseAction>False</IsDialogCloseAction>
