          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>var contact = (IContact) BindingSource.Current;

QFLocationCodeList.Items.Clear();
List&lt;Sage.SalesLogix.Web.EnabledLanguage&gt; cultures = Sage.SalesLogix.PickLists.PickListHelper.GetLanguageList&lt;Sage.SalesLogix.Web.EnabledLanguage&gt;(Sage.SalesLogix.PickLists.PickListHelper.LanguageListTypes.Cultures);
cultures.ForEach(ci =&gt; QFLocationCodeList.Items.Add(new ListItem(text: ci.DisplayText, value: ci.CultureCode.ToLowerInvariant())));
QFLocationCodeList.Items.Insert(0, new ListItem(text: string.Empty, value: null));
if (!string.IsNullOrWhiteSpace(contact.LocationCode)) {
	QFLocationCodeList.SelectedValue = contact.LocationCode.Trim();
} else {
	QFLocationCodeList.SelectedIndex = 0;
}
