      <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>    var optionSvc = Sage.Platform.Application.ApplicationContext.Current.Services.Get&lt;Sage.SalesLogix.Services.ISystemOptionsService&gt;(true);
    if(optionSvc.BaseCurrency != null) {
		  if(!numRate_lbl.Text.Contains(optionSvc.BaseCurrency)) {
	        numRate_lbl.Text = numRate_lbl.Text + "(= 1 " +  optionSvc.BaseCurrency + ")";
		  }
	    if(String.Equals(optionSvc.BaseCurrency,txtCurrencyCode.Text,StringComparison.InvariantCultureIgnoreCase)){
		      numRate.Enabled = false;
			    txtCurrencyCode.Enabled = false;
     	}	
	  }
	  
	var clientContextService = PageWorkItem.Services.Get&lt;Sage.Platform.WebPortal.Services.ClientContextService&gt;();
	if (clientContextService == null)
		return;
	clientContextService.CurrentContext["CURRENCYCODE"] = txtCurrencyCode.Text;
	var isDexEnabled = Sage.SalesLogix.BusinessRules.BusinessRuleHelper.IsDexEnabled();
	if(isDexEnabled)
	{
		numRate_lbl.Visible = false;
		numRate.Visible = false;	
		chkCurrency.Visible = false;
		chkAutomaticDownload.Visible = false;
	}
	

</CSharpCodeSnippet>
        </CSharpSnippetActionItem>
