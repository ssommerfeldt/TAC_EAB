        <CSharpSnippetActionItem>
          <MethodName>quickformload0</MethodName>
          <CSharpCodeSnippet>if (BindingSource != null)
{
    var discountChargeItem = BindingSource.Current as IDiscountChargeItem;
    var salesOrder = GetParentEntity() as ISalesOrder;
    var salesOrderItem = GetParentEntity() as ISalesOrderItem;
    var quote = GetParentEntity() as IQuote;
    var quoteItem = GetParentEntity() as IQuoteItem;
	var invoice = GetParentEntity() as IERPInvoice;
	var receivable = GetParentEntity() as IERPReceivable;
	
    if (salesOrder != null)
    {
        discountChargeItem.SODistributedCharge = salesOrder;
		BindingSource.Bind();
    }
    else if (salesOrderItem != null)
    {
        discountChargeItem.SOItemDistributedCharge = salesOrderItem;
        BindingSource.Bind();
    }
    else if (quote != null)
    {
        discountChargeItem.Quote = quote;
        BindingSource.Bind();
    }
    else if (quoteItem != null)
    {
        discountChargeItem.QuoteItem = quoteItem;
        BindingSource.Bind();
    }
	else if(invoice != null)
    {
        txtName.ReadOnly = true;
		txtReasonCode.ReadOnly = true;
		curAmount.ReadOnly = true;
		pklCalculationMethodCode.ReadOnly = true;
		txtDescription.ReadOnly = true;
		curRate.ReadOnly = true;
		txtDocUnitAdjustmentAmount.ReadOnly = true;
		txtUnitAdjustmentAmount.ReadOnly = true;
		//btnOK.Visible = false;
    }
	else if(receivable != null)
    {
        txtName.ReadOnly = true;
		txtReasonCode.ReadOnly = true;
		curAmount.ReadOnly = true;
		pklCalculationMethodCode.ReadOnly = true;
		txtDescription.ReadOnly = true;
		curRate.ReadOnly = true;
		txtDocUnitAdjustmentAmount.ReadOnly = true;
		txtUnitAdjustmentAmount.ReadOnly = true;
		//btnOK.Visible = false;
    }
}

</CSharpCodeSnippet>
